#places_index {
  .col-xs-12 {
    padding: 0px;
  }

  #places {
    .navbar-inverse {
      border: 0px;
    }
  }

  .alert {
    margin: 0px;
    z-index: 232;
    top: 50px;
  }

  .navbar {
    margin: 0;
  }

  .navbar-header {
    .col-xs-2,
    .col-xs-3,
    .col-xs-7 {
      padding: 0;
      min-height: 50px;
    }
  }

  .userpic {
    min-height: 50px;

    &:hover {
      cursor: pointer;
    }

    a {
      color: #fff;
      padding-top: 5px;
      display: block;

      &:hover {
        text-decoration: none;
      }
    }

    img {
      width: 38px;
      height: 38px;
      border-radius: 3px;
      border: 2px solid #fff;
    }

    ul.dropdown-menu {
      left: initial;
      right: -5px;
      border-top-left-radius: 0px;
      border-top-right-radius: 0px;
      background-color: #222;
      border: 0px;
      margin-top: 0;

      a:hover,
      a:focus {
        background-color: #ddd;
        color: #222;
      }
    }
  }

  .filter {
    position: relative;
    top: 51px;
    background-color: #E8E8E8;

    .loc-group.btn-group {
      padding: 5px 10px;

      div {
        padding: 0;
      }

      .btn {
        padding-top: 0px;
        font-size: 14px;

        &.btn-loc {
          border-top-right-radius: 0;
          border-bottom-right-radius: 0;
        }

        &.update-loc {
          border-top-left-radius: 0;
          border-bottom-left-radius: 0;
        }
      }
    }

    span.twitter-typeahead {
      width: 100%;
    }

    .col-xs-12 {
      padding: 5px 10px;
      background-color: #E8E8E8;

      .btn-block {
        border-radius: 2px;
        background-color: #D1D1D1;
        font-size: 14px;
        padding: 3px;
      }

      input[type="text"],
      .fake-input {
        border: 0;
        border-radius: 2px;
        padding: 5px 25px 5px 10px;
        height: 28px;
        font-size: 12px;
        background-image: image-url('search.png');
        background-repeat: no-repeat;
        background-size: 14px;
        background-position: 98% 50%;
        width: 100%;
      }

      .fake-input {
        font-weight: normal;
        overflow: hidden;
      }

      .col-xs-8,
      .col-xs-4 {
        padding: 0;
      }

      .filter-params {
        text-align: right;
      }

      &:last-of-type {
        padding-bottom: 10px;
      }

    }

    .btn-group {
      padding: 0 0 0 4px;

      .btn {
        padding: 5px 4px 0;
        font-size: 10px;
        border-radius: 2px;
        background-color: #D1D1D1;
        height: 28px;

        &:first-of-type {
          margin-right: 5px;
        }
      }
    }

  }

  .headline {
    padding: 12px 0 0;
    text-align: center;
    font-size: 16px;
    font-weight: 100px;
    color: #fff;
  }

  .navbar-brand {
    padding: 8px 0px 15px 15px;
    img {
      height: 100%;
    }
  }

  .content {
    background-color: #E8E8E8;
    position: absolute;
    top: 130px;
    width: 100%;
    min-height: 100vh;
  }

  .search-content {
    background: #fff;
    width: 100%;

    .results {
      padding: 5px 0 0;

      .result {
        list-style-type: none;
        border-bottom: 1px solid #9A9A9A;
        padding: 6px 10px;
      }

      .kind {
        float: right;
        text-transform: uppercase;
        font-size: 10px;
      }

      a,li {
        color: #8e8d8d;

        &:hover {
          color: #000;
          text-decoration: none;
          cursor: pointer;
        }
      }

    }

  }

  .center-block {
    text-align: center;

    p {
      margin-top: 20%;
      color: #333;
    }

    i.fa-spin {
      font-size: 4em;
      color: #d7d7d7;
    }
  }

  .list {
    padding: 0px;
    margin: 0px;
  }

  .list-item {
    border-radius: 0px;
    background-color: #E8E8E8;
    border-color: #BBBBBB;
    border-right: 0px;
    border-left: 0px;
    padding: 6px 20px;
    border-top: 1px solid #ABABAB;

    .place-name {
      padding: 0;

      h4 {
        margin: 0px;
        font-weight: 400;
        font-size: 16px;
        width: 86%;
        display: inline-block;
      }
    }

    .place-distance {
      padding: 0;
      text-align: right;
    }

    .place-info {
      padding: 0;
    }

    a{
      color: #E60000;
      &:hover {
        text-decoration: none;
      }
    }

    span {
      color: #9F9F9F;
      font-size: 11px;
    }

    &:last-child {
      border-bottom: 0;
    }

    .cuisines {
      margin: 0px;

      span {
        color: #89898D;
        font-size: 12px;
        font-weight: 600;
      }

      i {
        font-size: 8px;
      }
    }

    .address {
      margin: 0;
    }
  }

  .top-item {
    margin: 0px;
    padding: 5px 0 0;

    p {
      margin-bottom: 0;
    }

    .votes {
      width: 10%;
      display: inline-block;
      font-size: 10px;
      color: #E50000;

      span {
        text-align: center;
        font-size: 13px;
        color: #fff;
        min-width: 25px;
        height: 25px;
        background-color: #E50000;
        border-radius: 2px;
        display: inline-block;
        padding-top: 2px;
      }
    }

    .item-name {
      position: absolute;
      width: 88%;
      display: inline-block;
      font-size: 18px;
      color: #5C5C5C;
      font-weight: 700;
    }
  }

  .more-top-items {
    color: #878787;
    font-size: 12px;

    span {
      text-transform: uppercase;
      color: #333;
    }

  }

  .friend-likes {
    padding: 10px 0 0 0;
    text-align: right;

    span {
      font-size: 10px;
      color: #333;
    }

    .fb-pic {
      height: 30px;
      width: 30px;
      border-radius: 3px;

      &:first-of-type {
        margin-left: 5px;
      }
    }
  }

  #searchModal {
    .modal-dialog {
      margin: 0;
      display: flex;
      width: 100%;
      height: 100%;
    }

    .modal-content {
      width: 100%;
      margin: 0;
      padding: 0;
      border: 0;

      span.x {
        font-size: 35px;
      }
    }

    .modal-header {
      padding-bottom: 5px;
      border-bottom: 1px solid #9A9A9A;

      input {
        margin-top: 5px;
        border: 1px solid #ccc;
      }
    }

    .modal-body {
      padding-top: 0;
    }
  }

}

/* Places show screen styles */

#places_show{
  background-color: #E8E8E8;
  min-height: 100vh;

  .col-xs-12 {
    padding: 0px;
  }

  .header {
    height: 120px;
    padding-bottom: 10px;

    h2 {
      margin: 5px 0;
      color: #fff;
      font-weight: 400;
      font-size: 22px;
      text-shadow: 2px 2px 2px #000;
    }

    p {
      margin: 3px 0;
      color: #fff;
      font-size: 14px;
      font-weight: 400;
      text-shadow: 2px 2px 1px #000;
      a {
        color: #fff;
        text-decoration: none;
      }
    }

    .col-xs-12 {
      &:first-of-type {
        padding: 0px;
        clear: both;
      }

      &:last-of-type {
        height: 60%;
        padding: 10px;
        margin-top: 8px;
        //box-shadow: 0 -62px 60px rgba(141, 141, 141, 0.7)inset;
      }
    }

    .col-xs-1 {
      padding: 0;
    }

    .col-xs-11 {
      text-align: right;
      padding: 5px 15px;

      i {
        padding-right: 6px;
      }
    }

  }

  .back-link {
    padding: 0px 15px 0px 8px;
    color: #F30000;
    border: 0px;
    background: none;
    text-shadow: 2px 2px 2px #8E8E8E;
    &:hover, &:active, &:focus {
      border: 0px;
      color: #D32;
      box-shadow: none;
    }

    i {
      font-size: 50px;
    }
  }

  .divider {
    height: 28px;
    padding: 5px 0;
    margin-bottom: 5px;
    background-color: #4A4A4A;
    margin: 0;

    .col-xs-7 {
      padding: 0 0 0 15px;
      text-align: left;

      p {
        text-transform: uppercase;
        font-weight: bold;
        font-size: 12px;
        color: #fff;
      }
    }

    .col-xs-5 {
      padding-left: 0;
      text-align: right;

      label {
        display: inline-block;
        position: relative;
        top: -3px;
        padding-right: 5px;
        font-size: 10px;
        text-transform: uppercase;
        font-weight: bold;
        color: #fff;
        cursor: pointer;
      }
    }


    .roundedOne {
      width: 18px;
      height: 18px;
      position: relative;
      margin: 0;
      top: -1px;
      display: inline-block;
      background: #fcfff4;
      background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #fcfff4), color-stop(40%, #dfe5d7), color-stop(100%, #b3bead));
      background: -webkit-linear-gradient(top, #fcfff4 0%, #dfe5d7 40%, #b3bead 100%);
      background: -moz-linear-gradient(top, #fcfff4 0%, #dfe5d7 40%, #b3bead 100%);
      background: linear-gradient(top, #fcfff4 0%, #dfe5d7 40%, #b3bead 100%);
      -webkit-border-radius: 50px;
      -moz-border-radius: 50px;
      -ms-border-radius: 50px;
      border-radius: 50px;
      -webkit-box-shadow: inset 0px 1px 1px white, 0px 1px 3px rgba(0, 0, 0, 0.5);
      -moz-box-shadow: inset 0px 1px 1px white, 0px 1px 3px rgba(0, 0, 0, 0.5);
      box-shadow: inset 0px 1px 1px white, 0px 1px 3px rgba(0, 0, 0, 0.5);
      label {
        width: 12px;
        height: 12px;
        cursor: pointer;
        position: absolute;
        left: 3px;
        top: 3px;
        background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #222222), color-stop(100%, #45484d));
        background: -webkit-linear-gradient(top, #222222 0%, #45484d 100%);
        background: -moz-linear-gradient(top, #222222 0%, #45484d 100%);
        background: linear-gradient(top, #222222 0%, #45484d 100%);
        -webkit-border-radius: 50px;
        -moz-border-radius: 50px;
        -ms-border-radius: 50px;
        border-radius: 50px;
        -webkit-box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.5), 0px 1px 0px white;
        -moz-box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.5), 0px 1px 0px white;
        box-shadow: inset 0px 1px 1px rgba(0, 0, 0, 0.5), 0px 1px 0px white;
        &:after {
          content: '';
          width: 10px;
          height: 10px;
          position: absolute;
          top: 0.5px;
          left: 1px;
          background: #00bf00;
          background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #00bf00), color-stop(100%, #009400));
          background: -webkit-linear-gradient(top, #00bf00 0%, #009400 100%);
          background: -moz-linear-gradient(top, #00bf00 0%, #009400 100%);
          background: linear-gradient(top, #00bf00 0%, #009400 100%);
          filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
          opacity: 0;
          -webkit-border-radius: 50px;
          -moz-border-radius: 50px;
          -ms-border-radius: 50px;
          border-radius: 50px;
          -webkit-box-shadow: inset 0px 1px 1px white, 0px 1px 3px rgba(0, 0, 0, 0.5);
          -moz-box-shadow: inset 0px 1px 1px white, 0px 1px 3px rgba(0, 0, 0, 0.5);
          box-shadow: inset 0px 1px 1px white, 0px 1px 3px rgba(0, 0, 0, 0.5);
        }
        &:hover::after {
          filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=30);
          //opacity: 0.3;
        }
      }
      input[type=checkbox] {
        visibility: hidden;
        &:checked + label:after {
          filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=100);
          opacity: 1;
        }
      }
    }
  }

  .content {
    background-color: #E8E8E8;
    h4.heading {
      text-transform: uppercase;
      text-decoration: underline;
      text-align: center;
      color: #424141;
      font-weight: 300;
      margin-top: 2px;
    }
  }

  .friend-likes {
    text-align: center;
    border-bottom: 1px solid #9f9f9f;
    margin-bottom: 10px;

    p {
      font-size: 14px;
      color: #6B5C6B;
      font-weight: 200;
      margin-bottom: 5px;
    }

    .fb-pic {
      height: 30px;
      width: 30px;
      border-radius: 3px;
    }
  }

  .nav-tabs {
    margin: 0 6px;
    border: 0;
    border-bottom: 1px solid #a5a5a5;
    li {
      padding: 0px 3px;
      a{
        margin-bottom: 10px;
        margin-right: 0;
        padding: 4px;
        border-color: #B4B4B4;
        background-color: #D8D8D8;
        color: #181619;
        border-radius: 3px;
        font-size: 14px;
        font-weight: 400;
        box-shadow: #c5c5c5 1px 1px 1px 0px;

        span.fa {
          display: none;
          position: absolute;
          left: 40%;
          color: #a5a5a5;
          font-size: 28px;
          top: 72%;
        }

        &[href='#top5'] {
          i {
            color: #333;
            font-size: 16px;
          }
          span {
            left: 25%;
          }
        }

      }

      &.active a {
        background-color: #F30000;
        color: #fff;

        &[href='#top5'] i {
          color: #fff;
        }

        span {
          display: inline-block;
        }
      }
    }
  }

  .tab-pane {
    padding: 0px 10px 30px 10px;
    .row {
      margin: 0px;
      width: 100%;
    }

    .tab-header {
      font-size: 16px;
      text-align: center;
      color: #333;
    }
  }

  .item {
    border: 1px solid #AAAAAA;
    border-radius: 3px;
    padding:8px 0 4px;
    margin-bottom: 4px;
    background-color: #FFF;
    min-height: 54px;

    .col-xs-8 {
      padding: 0 0 0 12px;
    }

    .col-xs-1 {
      padding: 0 8px;
    }

    p {
      color: #4A4A4A;

      &.name {
        margin: 0;
        font-size: 14px;
        font-weight: 600;
      }

      &.more-info {
        margin: 0;
        span.price {
          font-size: 11px;
          padding-left: 5px;
          color: #7E7E7E;
          font-weight: normal;

          i {
            font-size: 9px;
          }
        }
      }

      &.votes {
        width: 95%;
        display: inline-block;
        color: #F10000;
        text-align: center;
        position: absolute;
        font-size: 10px;
        font-weight: 400;
        span {
          font-weight: 600;
          font-size: 14px;
          padding: 2px;
          color: #fff;
          display: block;
          border-radius: 4px;
          background-color: #F10000;
        }
      }

      &.like_it {
        margin-top: 3px;
        margin-bottom: 0;
      }

      span.desc {
        padding-left: 4px;
        padding-top: 5px;

        a {
          font-weight: 400;
          font-size: 9px;
          color: #F30000;
          &.less {
            display: none;
          }
          &:hover {
            cursor: pointer;
            text-decoration: none;
          }
        }
      }


    }


    .col-xs-3 {
      text-align: right;
      padding: 0 6px 0 2px;
    }

    .btn {
      padding: 3px 4px;
      min-width: 60px;
      background-color: #f8f5f5;
      box-shadow: #c5c5c5 1px 1px 1px 0px;
      border-radius: 3px;

      &.like {
        border-color: #9B9B9B;
        color: #333;
      }

      &.unlike {
        background-color: #fff;
        border-color: #F30000;
        color: #F30000;
      }
    }


  }
    .no-tags {
      margin-top: 10px;
    }

  .item_desc {
    display: none;
    padding: 5px 0;

    p {
      margin: 0;
      padding: 0 10px;
      font-size: 12px;
      font-weight: 300;
    }
  }

  .fb_info {
    padding: 6px 8px;
  }

  .friend_likes {
    margin: 0;
    font-size: 12px;
    font-weight: 400;
    color: #BCBCBC;

    img.fb-pic {
      &:first-child {
        margin-left:2px;
      }
      height: 27px;
      width: 27px;
      border: 1px solid #B0B0B0;
      margin-right: 1px;
    }
  }

  .tab-pane {
    &#top5 {
      .winner {
        text-align: center;
        font-size: 16px;
        font-weight: bold;
        color: #5C5C5C;
      }
    }

    .nav-tabs {
      padding: 10px 0 5px;
      border: 0;
      margin: 0;

      li {
        &.active a {
          &:after {
            content: none;
          }
          background-color: #51914C;
        }
        a {
          background-color: #666666;
          color: #fff;
          padding: 1px 14px;
        }
      }
    }

    .tab-pane {
      padding: 0;
    }
  }

}
